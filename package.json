{
  "name": "@aeronus/objectdeepsearch",
  "version": "1.0.0",
  "description": "Simple module for recursive search in objects and arrays",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
