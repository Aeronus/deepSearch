  "bugs": {
    "url": "https://github.com/Aeronus/objectDeepSearch/issues"
  },
  "homepage": "https://github.com/Aeronus/objectDeepSearch#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
